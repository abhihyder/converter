{
    "name": "hyder/converter",
    "description": "A Laravel package that provides an easy-to-use interface to convert PDF files to images using the Imagick PHP extension.",
    "keywords": [
        "pdf-to-image",
        "image-to-pdf",
        "pdf",
        "image",
        "converter",
        "hyder",
        "laravel"
    ],
    "authors": [
        {
            "name": "Tofayel Hyder Abhi",
            "email": "tofayelhyder71@gmail.com",
            "role": "Developer"
        }
    ],
    "version": "1.0.1",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hyder\\Converter\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hyder\\Converter\\ConverterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
